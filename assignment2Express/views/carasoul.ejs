<div class="container mt-5">
    <h2 class="text-center mb-4">Brand Carousel</h2>
    <div id="brandCarousel" class="carousel slide" data-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="brand1.png" class="d-block w-100" alt="Brand 1 Logo">
        </div>
        <div class="carousel-item">
          <img src="brand2.png" class="d-block w-100" alt="Brand 2 Logo">
        </div>
        <div class="carousel-item">
          <img src="brand3.png" class="d-block w-100" alt="Brand 3 Logo">
        </div>
        <!-- Add more carousel items as needed -->
      </div>
      <a class="carousel-control-prev" href="#brandCarousel" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#brandCarousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div>
  
  <script>
    // Auto start carousel
    $('.carousel').carousel({
      interval: 2000 // Change the interval (in milliseconds) as needed
    });
  </script>