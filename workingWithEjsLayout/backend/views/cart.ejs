<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Price</th>
            <th>Quantity</th>
            <!-- Add more columns if needed -->
        </tr>
    </thead>
    <tbody>
        <% meats.forEach(meat => { %>
            <tr> 
                <td><%= meat.name %></td>
                <td><%= meat.price %></td>
                <td><%= meat.quantity %></td>
                
                <!-- Add more columns if needed -->
            </tr>
        <% }); %>
    </tbody>
    <tfoot>
        <!-- Add footer rows if needed -->
    </tfoot>
</table>
